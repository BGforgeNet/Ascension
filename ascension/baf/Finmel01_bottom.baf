IF
  Global("feedback","LOCALS",1)
THEN
  RESPONSE #100
    SetGlobal("feedback","LOCALS",0)
    SetInterrupt(FALSE)
    ClearActions(Myself)
    PlayDead(90)
    SetInterrupt(TRUE)
END

