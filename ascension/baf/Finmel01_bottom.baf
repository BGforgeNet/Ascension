IF
  Global("FinalFight","AR6200",8)
  Global("allfive","GLOBAL",1)
  Global("BalthazarFights","GLOBAL",1)
  HPLT(Myself,20)
THEN
  RESPONSE #100
    SetGlobal("allfive","GLOBAL",2)
    ClearAllActions()
    StartCutSceneMode()
    StartCutScene("cut233ga")
END

